{
  "name": "grainstack",
  "version": "0.0.4",
  "license": "MIT",
  "description": "A way of making three-tier apps with a focus on  in browser-native JS.",
  "keywords": [
    "small",
    "minimal",
    "fine-grained",
    "reactive",
    "state management",
    "frp",
    "fp",
    "functional",
    "functional-reactive-programming",
    "front-end",
    "framework",
    "vanilla",
    "grain",
    "grainstack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/grainstackdev/grainstack.git"
  },
  "exports": {
    "node": {
      "import": {
        ".": "./esm/index.js",
        "./reactivity": "./esm/reactivity.js",
        "./history": "./esm/history.js",
        "./routing": "./esm/routing.js",
        "./hyperscript": "./esm/grainstack-hyperscript.js",
        "./html-tag": "./esm/html-tag.js"
      },
      "require": {
        ".": "./cjs/index.js",
        "./reactivity": "./cjs/reactivity.js",
        "./history": "./cjs/history.js",
        "./routing": "./cjs/routing.js",
        "./hyperscript": "./cjs/grainstack-hyperscript.js",
        "./html-tag": "./cjs/html-tag.js"
      }
    }
  },
  "files": [
    "./docs",
    "./dist",
    "./examples"
  ],
  "scripts": {
    "build": "prettier --write src && rm -rf cjs && rm -rf esm && flow-remove-types src --out-dir esm && babel esm --out-dir cjs && flow-copy-source src esm && absolute-import-paths --glob \"esm/**/*.{js,mjs,cjs}\"",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "absolute-import-paths": "^1.0.3",
    "flow-bin": "0.195.1",
    "flow-copy-source": "^2.0.9",
    "flow-remove-types": "^2.197.0",
    "prettier": "^2.8.1"
  },
  "dependencies": {
    "commentless-tag": "^1.0.3",
    "history-events": "^1.0.4",
    "htm": "^3.1.1",
    "qss": "^2.0.0",
    "regexparam": "^2.0.1"
  },
  "type": "module"
}
